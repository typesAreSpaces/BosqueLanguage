//
//This is a bosque test/benchmark for a simple max function.
//Expected output is: 20
//

namespace NSMain;

function max(x: Int, y: Int): Int
{
    var! z = 10;
    var! z2 = 2;

    if(x > 0){
        z = z + 1;
        z2 = z + z2;
    }
    else {
        z = z + 2;
        z2 = z;
    }
    z = z + z2 + 1;

    return z;
}

entrypoint function main(): Int {
    var x = 10;
    var y = 20;

    return max(x, y);
}